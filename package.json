{
  "name": "todo-app-profesional",
  "version": "1.0.0",
  "description": "Una aplicación profesional de gestión de tareas con análisis avanzados y PWA",
  "keywords": ["react", "todo", "productivity", "pwa", "analytics"],
  "homepage": "https://tu-usuario.github.io/todo-app",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server": "json-server --watch db.json --port 3001 --delay 500",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "gh-pages": "^5.0.0",
    "json-server": "^0.17.4"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}